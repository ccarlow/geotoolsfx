<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import dnddockfx.*?>

<DockPane fx:id="configMapLayerDockPane" title="Config Map Layer Edit" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="geotoolsfx.ui.controller.ConfigEditMapLayerController">
	<content>
		<GridPane>
			<Label text="Title"/>
			<TextField fx:id="title" GridPane.columnIndex="1"/>
	
			<Label text="Feature Collection" GridPane.rowIndex="1"/>
			<ComboBox fx:id="featureCollection" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
		</GridPane>
		
		<fx:include fx:id="styledLayerDescriptor" source="StyledLayerDescriptor.fxml"/>
	</content>
</DockPane>