<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import dock.*?>
<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="geotoolsfx.ui.controller.StylesController">	
	<TreeView fx:id="treeView" VBox.vgrow="ALWAYS">
		<root>
			<TreeItem value="root" expanded="true"/>
		</root>
		<contextMenu>
			<ContextMenu fx:id="contextMenu">
				<items>
					<MenuItem text="Open" onAction="#openTreeItem"/>
				</items>
			</ContextMenu>
		</contextMenu>
	</TreeView>
</VBox>