<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import dnddockfx.*?>

<DockPane fx:id="configsDockPane" title="Configs" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="geotoolsfx.ui.controller.ConfigsController">	
	<content>
		<TreeView fx:id="treeView">
			<root>
				<TreeItem fx:id="treeRoot" value="Configs" expanded="true">
					
				</TreeItem>
			</root>
			<contextMenu>
				<ContextMenu fx:id="contextMenu">
					<items>
						<MenuItem text="Open" onAction="#openConfig"/>
					</items>
				</ContextMenu>
			</contextMenu>
		</TreeView>
	</content>
</DockPane>